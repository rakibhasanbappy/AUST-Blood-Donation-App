<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bappy.austblooddonationapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AUSTBloodDonationApp">
        <activity android:name=".activities.SearchPage"></activity>
        <activity android:name=".activities.Dashboard" />
        <activity android:name=".activities.SignInScreen" />
        <activity android:name=".activities.SignUpScreen" />
        <activity android:name=".activities.StartingScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />
    </application>

</manifest>